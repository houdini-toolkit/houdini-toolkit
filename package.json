{
  "name": "houdini-toolkit",
  "version": "1.0.0-beta.0",
  "description": "Toolkit for Houdini api (Cascading Style Sheets)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "lint": "npx eslint src/**/* --fix",
    "lint:test": "npx eslint test/**/* --fix",
    "prebuild": "npm run lint",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houdini-toolkit/houdini-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/houdini-toolkit/houdini-toolkit/issues"
  },
  "keywords": [
    "houdini",
    "houdini css",
    "css",
    "toolkit",
    "houdini api"
  ],
  "author": "Anton Maklakov",
  "license": "MIT",
  "engines": {
    "node": ">=10.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "node-html-parser": "^6.1.12"
  }
}
